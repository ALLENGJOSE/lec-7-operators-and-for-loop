import java.util.Scanner;
public class Main {
	
	public static void main(String[] args) {
		Scanner s = new Scanner(System.in);
        int n = s.nextInt();
        
        int reversednumber =0;
        while(n >0){
            int lastdigit = n%10;
            reversednumber = reversednumber * 10 + lastdigit;
            n = n/10;
        }
        System.out.print(reversednumber);

	}
}
